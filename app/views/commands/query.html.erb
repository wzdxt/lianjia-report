<h2><%= @title %></h2>
<table>
  <thead>
  <tr>
    <% @data.all.columns.each do |title| %>
        <th><%= title %></th>
    <% end %>
  </tr>
  </thead>
  <tbody>
  <% @data.all.each do |record| %>
      <tr>
        <% record.values.each do |val| %>
            <td><%= val %></td>
        <% end %>
      </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @data %>
