<h2><%= @title %></h2>
<table>
  <thead>
  <tr>
    <% @data.select_values.each do |col| %>
        <th><%= @data.model.human_attribute_name col %></th>
    <% end %>
  </tr>
  </thead>
  <tbody>
  <% @data.all.each do |record| %>
      <tr>
        <% @data.select_values.each do |col| %>
            <td><%= record[col] %></td>
        <% end %>
      </tr>
  <% end %>
  </tbody>
</table>

<%= paginate @data %>
