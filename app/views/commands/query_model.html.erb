<h2><%= @title %></h2>
<div class="model-query-result">
  <table class="result">
    <thead>
    <tr>
      <% @data.select_values.each do |col| %>
          <th class="col-<%= col %>"><%= @data.model.human_attribute_name col %></th>
      <% end %>
    </tr>
    </thead>
    <tbody>
    <% @data.all.each do |record| %>
        <tr>
          <% @data.select_values.each do |col| %>
              <td class="col-<%= col %>"><%= record[col] %></td>
          <% end %>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>

<%= paginate @data %>
